<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
	<sitecore role:require="Standalone or ContentManagement">
		<pipelines>
			<mvc.requestBegin>
				<processor type="LionTrust.Foundation.Navigation.Pipelines.GatedAccessProcessor, Liontrust.Foundation.Navigation"
								patch:after="processor[@type='Sitecore.Mvc.Analytics.Pipelines.MvcEvents.RequestBegin.StartTracking, Sitecore.Mvc.Analytics']">
				</processor>
			</mvc.requestBegin>
		</pipelines>
	</sitecore>
</configuration>